#include <stdio.h>

typedef struct temps {
    unsigned heures;
    unsigned minutes;
    double secondes;
    struct temps *next;
} time;


int main(void)
{
    time t = { 1 , 02 , 0.53};
    printf("%d, %d, %f\n", t.heures, t.minutes, t.secondes);
    t.heures = 23;
    printf("%d, %d, %f\n", t.heures, t.minutes, t.secondes);
    time *p = &t;
    p->heures = 2;
    printf("%d, %d, %f\n", t.heures, t.minutes, t.secondes);
    return 0;
}
